{
  "name": "energy-drink-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "analyze": "ANALYZE=true npm run build",
    "security:audit": "npm audit --audit-level high",
    "security:snyk": "snyk test",
    "security:snyk:monitor": "snyk monitor",
    "security:scan": "node scripts/security-scan.js",
    "security:lint": "npm run lint -- --format=json --output-file=security-lint-results.json",
    "security:report": "node scripts/generate-security-report.js",
    "security:install": "npm install --save-dev @snyk/cli audit-ci",
    "security:full": "npm run security:scan && npm run security:audit",
    "security:precommit": "npm run security:lint && npm run security:audit",
    "security:validate": "node scripts/validate-security-gates.js",
    "prepare": "husky",
    "semantic-release": "semantic-release",
    "backup": "node scripts/backup-data.js",
    "restore": "node scripts/restore-data.js",
    "validate-data": "node scripts/validate-data.js",
    "build:fast": "next build --no-lint --no-type-check",
    "dev:fast": "next dev --turbo"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/nextjs": "^10.29.0",
    "@sentry/replay": "^7.116.0",
    "@types/redis": "^4.0.10",
    "@vercel/analytics": "^1.6.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "color-contrast-checker": "^2.1.0",
    "critters": "^0.0.23",
    "express-rate-limit": "^8.2.1",
    "lucide-react": "0.556.0",
    "mermaid": "^11.12.2",
    "next": "^15.5.7",
    "next-csrf": "^0.2.1",
    "next-intl": "^4.5.8",
    "playwright": "^1.57.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "redis": "^5.10.0",
    "tailwind-merge": "^3.4.0",
    "web-vitals": "^5.1.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "24.10.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.15",
    "cross-env": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.1.6",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "snyk": "^1.1301.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.15",
    "webpack-bundle-analyzer": "^5.1.0"
  }
}
